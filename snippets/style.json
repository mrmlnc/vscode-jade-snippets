{
  "link-stylesheet": {
    "prefix": "link stylesheet",
    "body": "link(rel='stylesheet', type='text/css', href='$1')",
    "description": "link stylesheet"
  },
  "style": {
    "prefix": "style",
    "body": "style.\n\t$1\n$0",
    "description": "style."
  },
  "style include": {
    "prefix": "style:include",
    "body": "style\n\tinclude ${1:./file.css}\n$0",
    "description": "style -> include"
  }
}
